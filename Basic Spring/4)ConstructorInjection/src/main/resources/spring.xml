<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="employee1" class="com.psl.bean.Employee">
		<constructor-arg name="empId" value="1"></constructor-arg>
		<constructor-arg name="empName" value="Rajesh"></constructor-arg>
	</bean>

	<bean id="employee2" class="com.psl.bean.Employee">
		<constructor-arg name="empId" value="2"></constructor-arg>
		<constructor-arg name="empName" value="Priyanka"></constructor-arg>
	</bean>
</beans>